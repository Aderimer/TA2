<div class="main-container">
    <% data.forEach(post => { %>
        <section class="img-sec">
            <a class="img-card" href="/bilde/<%= post._id %>">
                <h1><%= post.title %></h1>
                <img src="<%= s3 %><%= post.imageName %>" alt="<%= post.alt %>" height="auto" width="auto">
            </a>
        </section>
    <% }) %>

    

    <% if (prevPage !== null) { %>
        <a href="/?page=<%= prevPage %>" class="pagination">Forrige side</a>

    <% } %>
    <% if (nextPage !== null) { %>
        <a href="/?page=<%= nextPage %>" class="pagination">Neste Side</a>
    <% } %>




</div>
