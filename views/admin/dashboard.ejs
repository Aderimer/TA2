<div class="dash-wrapper">
    <h1>Dashboard</h1>
    
    <section class="dash-bilder">
        <h2>Legg ut nye bilder</h2>
        <a href="/admin/add-post" class="btn">+ Nytt bilde</a>
    </section>

    <div class="bilde-container">
        <% data.forEach(data => { %>

            <li>


                <div class="admin-post-controls">
                    <a href="/admin/edit-post/<%= data._id %>" class="btn">Rediger</a>

                    <form action="/admin/delete-post/<%= data._id %>?_method=DELETE" enctype="multipart/form-data" method="POST">
                        <input type="submit" value="Delete" class="btn-delete">
                    </form>
                    <h1><%= data.title %></h1>
                </div>

                <img src="<%= s3 %><%= data.imageName %>" width="175" height="175"/>

            </li>
         
        <% }) %>
    </div>


    <div class="register-container">
        <h1>Registrer ny Administrator</h1>

        <form action="/admin/register" method="POST">

            <label for="username"><b>Brukernavn: </b></label>
            <input type="text" placeholder="Brukernavn" name="username">

            <label for="password"><b>Passord: </b></label>
            <input type="password" placeholder="Passord" name="password">

            <input type="submit" value="Registrer" class="form-btn">

        </form>
    </div>
    
</div>